<odoo>

    <template id="cart_lines" inherit_id="website_sale.cart_lines">
        <xpath
            expr="//div[hasclass('css_quantity')]/t[contains(@t-if, 'not line._is_not_sellable_line()')]"
            position="attributes"
        >
            <attribute name="t-if" separator=" and " add="not line.event_ok" />
        </xpath>
    </template>

</odoo>
